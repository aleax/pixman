@@ -3878,7 +3878,7 @@
 			    idistx = 256 - distx;
 			    idisty = 256 - disty;
 			    
-			    b = bits + (y1)*stride;
+			    b = bits + (signed)((y1)*stride);
 			    x_off = x1;
 			    
 			    x1_out = (x1 < box.x1) | (x1 >= box.x2);
@@ -3944,7 +3944,7 @@
 			    idistx = 256 - distx;
